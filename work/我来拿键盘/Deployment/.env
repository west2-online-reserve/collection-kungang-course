# 数据库配置
DB_HOST=db
DB_PORT=3306
DB_USER=root
DB_PASSWORD=123456
DB_NAME=fzu_history

# Redis配置
REDIS_ADDR=redis:6379
REDIS_PASSWORD=123456

# 后端服务配置
BACKEND_PORT=8080
GIN_MODE=release

# 后端静态资源URL（用于Nginx代理）
BACKEND_STATIC_URL=http://backend:8080/static

# Nginx配置
NGINX_PORT=80

# 文件上传路径
UPLOAD_PATH=/app/static

# JWT密钥（生产环境请修改为强密钥）
JWT_SECRET=your_jwt_secret_key_here_please_change_in_production

# 跨域配置
ALLOWED_ORIGINS=http://localhost,http://localhost:80
